{
    "name": "gupta/laravel-installer-with-envato",
    "description": "Laravel installer with envato license verification setup",
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "Gupta\\LaravelInstallerWithEnvato\\": "src/"
        }
    },
    "authors": [
        {
            "name": "Kuntal Gupta",
            "email": "Kuntal1230@gmail.com"
        }
    ],
    "require": {
        "php": "^8.1",
        "jackiedo/dotenv-editor": "^2.1.0",
        "illuminate/contracts": "^10.0",
        "php-flasher/flasher-laravel": "^1.13",
        "spatie/laravel-markdown": "^2.3",
        "spatie/laravel-package-tools": "^1.14.0"
    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "providers": [
                "Gupta\\LaravelInstallerWithEnvato\\InstallerServiceProvider",
                "Jackiedo\\DotenvEditor\\DotenvEditorServiceProvider"
            ]
        }
    }
}
